<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <!-- PRODUCT TEMPLATE VIEW -->

        <record id="product_variant_multi_product_template_sequence_view" model="ir.ui.view">
            <field name="name">product.variant.multi.product.template.sequence.form</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='name']/.." position="after">
                    <group>
                        <field name="template_code"/>
                    </group>
                </xpath>
            </field>
        </record>

    </data>
</openerp>
